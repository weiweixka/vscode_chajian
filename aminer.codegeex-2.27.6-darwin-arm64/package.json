		"keybindings": [
			{
				"command": "codegeex.new-completions",
				"key": "Alt+\\",
				"when": "editorFocus && !editorReadonly"
			},
			{
				"command": "codegeex.translate-mode",
				"key": "Alt+ctrl+T",
				"when": "editorFocus && !editorReadonly"
			},
			{
				"command": "editor.action.inlineSuggest.commit",
				"key": "Tab",
				"when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorHoverFocused && !editorTabMovesFocus && editorFocus &&!editorReadonly"
			},
			{
				"command": "editor.action.inlineSuggest.acceptNextLine",
				"mac": "Cmd+down",
				"key": "ctrl+down",
				"when": "inlineSuggestionVisible && !editorHoverFocused && !editorTabMovesFocus && editorFocus &&!editorReadonly"
			},
			{
				"command": "codegeex.chat.toggle",
				"key": "alt+y",
				"mac": "cmd+y"
			},
			{
				"command": "codegeex.ghostComment.accept",
				"key": "ctrl+right",
				"mac": "cmd+right",
				"when": "!inlineSuggestionVisible && !editorReadonly && !codegeex.enableLocalMode && codegeex.enablecmd+right"
			},
			{
				"command": "codegeex.ghostComment",
				"key": "Alt+A",
				"when": "!codegeex.enableLocalMode"
			},
			{
				"command": "codegeex.inlineChat.input",
				"key": "ctrl+i",
				"mac": "cmd+i",
				"when": "editorFocus && !editorReadonly && !codegeex.enableLocalMode"
			},
			{
				"command": "codegeex.toolbar.add",
				"key": "ctrl+n",
				"mac": "cmd+n",
				"when": "view.codegeex-qa.visible && ((codegeex.navTab === 'chat' && codegeex.chatInputFocus) || (codegeex.navTab === 'agent' && codegeex.agentInputFocus) || (codegeex.navTab === 'toolbox' && codegeex.toolboxInputFocus))"
			},
			{
				"command": "codegeex.agent.stop",
				"key": "ctrl+shift+backspace",
				"mac": "cmd+shift+backspace",
				"when": "!codegeex.enableLocalMode"
			},
			{
				"command": "codegeex.agent.debugChat",
				"key": "ctrl+enter",
				"mac": "cmd+enter",
				"when": "!editorFocus && !codegeex.enableLocalMode"
			}
